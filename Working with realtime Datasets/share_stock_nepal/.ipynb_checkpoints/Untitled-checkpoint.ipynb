   "source": [
    "import os\n",
    "import glob\n",
    "import numpy as np\n",
    "import pandas as pd\n",
    "import seaborn as sns\n",
    "import plotly.graph_objs as go\n",
    "import matplotlib.pyplot as pl"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "6f003f30",
   "metadata": {},
   "source": [
    "# Merging all the available csv files to single dataframe"
   ]
  },
  {
   "cell_type": "raw",
   "id": "6e44cb1b",
   "metadata": {},
   "source": [
    "basedir=\"D:/A_DataScience/Working with realtime Datasets/share_stock_nepal/Nepali_Stock_Market _Data__2012-2020\"\n",
    "os.chdir(basedir)\n",
    "dirs=os.listdir('.')\n",
    "extension = 'csv'\n",
    "columns=['S.No', 'Symbol', 'Conf.', 'Open', 'High', 'Low', 'Close', 'VWAP',\n",
    "       'Vol', 'Prev. Close', 'Turnover', 'Trans.', 'Diff', 'Range', 'Diff %',\n",
    "       'Range %', 'VWAP %', '52 Weeks High', '52 Weeks Low', '180 Days','Date']"
   ]
  },
  {
   "cell_type": "raw",
   "id": "d0b0f91f",
   "metadata": {},
   "source": [
    "dataframe=pd.DataFrame(columns=columns)\n",
    "for directory in dirs:\n",
    "    os.chdir(f\"{basedir}/{directory}\")\n",
    "    all_filenames = [i for i in glob.glob('*.{}'.format(extension))]\n",
    "    for f in all_filenames:\n",
    "        df=pd.read_csv(f)\n",
    "        df['Date']=f[:-4]\n",
    "        dataframe=pd.concat([dataframe,df])    \n",
    "os.chdir(basedir)\n",
    "dataframe.to_csv( \"data_csv.csv\", index=False, encoding='utf-8-sig')"
   ]
  },
